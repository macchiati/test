				<variable id="$JL">($JL $X)</variable>
				<variable id="$JT">($JT $X)</variable>
				<variable id="$JV">($JV $X)</variable>
				<variable id="$NS">($NS $X)</variable>
				<variable id="$NU">($NU $X)</variable>
				<variable id="$OP">($OP $X)</variable>
